{
  "expo": {
    "name": "AMU CALENDAR",
    "slug": "AMUCalendarAPP",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier":"com.amu.amucalendar",
      "buildNumber":"1.0.0"
    },
    "android": {
      "package":"com.amu.amucalendar",
      "versionCode":1,
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "description": "Your Destination for Holidays, Notices, Events & News Updates of  The Aligarh Muslim University"
  }
}
